<% if object.errors.any? %>
  <div id="error_explanation">
    <div class="alert alert-danger">
      Formuläret innehåller <%= pluralize(object.errors.count, "")%>  fel.
     </div>
    <ul>
      <% object.errors.messages.each do |attr, msg| %>
        <% msg.each do |val| %>
        <li><%= val %></li>
        <% end %>
      <% end %>
    </ul>
  </div>
<% end %>