<% @apikeys.each do |element| %>
 <li id="apikey-<%= element.id %>">
  <span class="application_name"><%= element.application_name %></span>
  <span class="application_key"><%= element.application_key %></span>
  <%= link_to "Radera", element, method: :delete,
                                       data: { confirm: "Är du säker på att applikationen ska raderas?" } %>
</li>
<% end %>

