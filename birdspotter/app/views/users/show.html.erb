<% provide(:title, @user.username) %>
<div class="col-xs-12 col-sm-5 col-sm-offset-1 col-md-5 col-sm-offset-1 col-lg-5 col-lg-offset-1">
  <h1>
    <%= @user.username %>
  </h1>
  <div>
     <%= render 'apikeys/apikey_form' %>
  </div>
  <div>
    <% if @user.apikeys.any? %>
        <ol class="apikeys">
          <%= render 'apikeys/apikey' %>
        </ol>
        <%= will_paginate @apikeys %>
        <% else %>
        <p>Du har inga apinycklar</p>
      <% end %>
      </ol>
  </div>
</div>
