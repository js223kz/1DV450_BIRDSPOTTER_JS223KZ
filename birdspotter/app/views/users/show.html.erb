<% provide(:title, @user.username) %>
<div class="row">
  <aside class="col-md-4">
    <section class="user_info">
      <h1>
        <%= @user.username %>
      </h1>
    </section>
    <section class="col-md-8">
      <% if @user.apikeys.any? %>
        <ol class="apikeys">
          <%= render @apikeys %>
        </ol>
        <%= will_paginate @apikeys %>
        <% else %>
        <p>Du har inga apinycklar</p>
      <% end %>
      </ol>
    </section>
  </aside>
</div>
